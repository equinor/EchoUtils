{
    "name": "@equinor/echo-utils",
    "version": "0.1.0",
    "description": "Utils and hooks for echo web project",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "source": "src/index.ts",
    "module": "true",
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w ",
        "start": "npm run build && node dist/index.js",
        "echo-publish": "npm publish --access public",
        "echo-test": "npm ci & jest --watchAll",
        "test": "jest",
        "test-coverage": "npm ci & jest --coverage --watchAll",
        "patch": "npm version patch",
        "major": "npm version major",
        "minor": "npm version minor",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
        "build-docs": "npx typedoc --out docs src/docs.ts"
    },
    "author": "Echo Core",
    "license": "MIT",
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "react-cool-onclickoutside": "^1.6.1"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "@testing-library/dom": "^7.29.4",
        "@testing-library/react": "^11.2.3",
        "@testing-library/react-hooks": "^5.1.0",
        "@types/jest": "^26.0.20",
        "@types/react": "^17.0.0",
        "@types/react-test-renderer": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-jest": "^26.6.3",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.2",
        "eslint-plugin-react-hooks": "^4.1.2",
        "jest": "^26.6.3",
        "jest-expo": "^40.0.1",
        "prettier": "^2.0.5",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-test-renderer": "^17.0.1",
        "rollup": "^2.34.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-delete": "^2.0.0",
        "rollup-plugin-dts": "^2.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-typescript": "^1.0.1",
        "rollup-plugin-typescript-paths": "^1.2.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "rollup-plugin-web-worker-loader": "^1.6.0",
        "typedoc": "^0.20.35",
        "typescript": "^4.1.2"
    },
    "files": [
        "dist/"
    ]
